{
  "name": "comicbox",
  "publisher": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.4.0"
  },
  "main": "dist/extension.js",
  "activationEvents": [
    "onLanguage:box",
    "onCommand:comicbox.compile",
    "onCommand:comicbox.showPreview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "comicbox.compile",
        "title": "Comicbox: Compile the current file"
      },
      {
        "command": "comicbox.showPreview",
        "title": "Comicbox: Show a preview of the current file"
      }
    ]
  },
  "dependencies": {
    "pdfkit": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.33"
  }
}
